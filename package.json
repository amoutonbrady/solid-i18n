{
  "name": "@amoutonbrady/solid-i18n",
  "version": "0.4.0",
  "description": "Smallest translation lib for solid",
  "author": "Alexandre Mouton-Brady <amoutonbrady@gmail.com>",
  "homepage": "https://github.com/amoutonbrady/solid-i18n#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amoutonbrady/solid-i18n.git"
  },
  "keywords": [
    "solid",
    "solidjs",
    "i18n",
    "l10n"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/esm/solid-i18n.js",
  "module": "dist/esm/solid-i18n.js",
  "types": "dist/types/solid-i18n.d.ts",
  "exports": {
    ".": {
      "solid": "./dist/source/solid-i18n.jsx",
      "import": "./dist/esm/solid-i18n.js",
      "browser": "./dist/esm/solid-i18n.js",
      "require": "./dist/esm/solid-i18n.js",
      "node": "./dist/esm/solid-i18n.js"
    }
  },
  "sideEffect": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "solid-js": "^1.0.0"
  },
  "devDependencies": {
    "rollup": "^2.52.7",
    "rollup-preset-solid": "^1.0.1",
    "typescript": "^4.3.5"
  }
}
